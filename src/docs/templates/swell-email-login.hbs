        border-right: 1px solid #000;
      }

      .footer-content .links span:last-child a {
            border-right: none;
        }

      .footer-content .descpiption {
        margin-top: 10px;
        padding: 0 5px;
      }

      .refer-friend p {
        margin: 20px;
        text-align: right;
      }

      .footer-content .descpiption .mail {
        text-decoration: underline;
        color: #000;
      }

        .brand-reviews p, .brand-reviews-stars {
            margin-top: 10px;
            margin-left: 5px;
        }

        .brand-follow-heading, .brand-follow-icons {
            text-align: center;
        }

        .brand-follow-icons a {
            margin-right: 13px;
        }

        .brand-follow-icons a img {
            vertical-align: middle;
        }
		.display-block {
			display: block;
		}
		.text-secondary {
			color: #CE3193;
		}
		.text-yellow {
			color: #FFF7B5;
		}
      /* ========== end custom styles ========== */
    </style>
    <!-- end desktop styles -->
    <!-- responsive styles -->
    <style type="text/css" media="only screen and (max-width: 600px)">
    @media screen and (max-width: 600px) {
        div,
        p,
        a,
        li,
        span,
        td { -webkit-text-size-adjust:none; }

        /* ========== utilities ========== */

        /* makes images responsive inside columns on mobile devices */
        .flex-image {
            max-width: 100% !important;
            height: auto;
        }

        .banner-img {
            width: 100%;
        }

        .flex-button {
            max-width:600px !important;
            width:100% !important;
        }

        .flex-button a {
            display: block !important;
            font-size: 18px !important;
        }

        .mobile-hide { display: none !important; }

        .desktop-hide {
            display: table !important;
            max-height: none !important;
            overflow: visible !important;
            float: none !important;
            font-size: 12px !important;
            line-height: 1.5 !important;
        }

        .desktop-hide td,
        td.desktop-hide {
            display: inline-table !important;
        }

        .desktop-hide img,
        img.desktop-hide {
            max-height: none !important;
            width: auto !important;
        }

        .desktop-hide .flex-image {
            width: 100% !important;
        }

        /* ========== end utilities ========== */

        /* ========== scaffolding ========== */
        .main,
        .row,
        .fluid-row,
        .md-fluid-row,
        .row-spacer,
        #navigation,
        .row .col-1,
        .row .col-2,
        .row .col-3,
        .row .col-4,
        .row .col-5,
        .row .col-6,
        .row .col-7,
        .row .col-8,
        .row .col-9,
        .row .col-10,
        .row .col-11,
        .col-12 { width: 100% !important; height: auto; }

        .fluid-row .col-1,
        .md-fluid-row .col-1 { width: 8.25% !important;}
        .fluid-row .col-2,
        .md-fluid-row .col-2 { width: 16.5% !important;}
        .fluid-row .col-3,
        .md-fluid-row .col-3 { width: 25% !important;}
        .fluid-row .col-4,
        .md-fluid-row .col-4 { width: 33% !important;}
        .fluid-row .col-5,
        .md-fluid-row .col-5 { width: 41.25% !important;}
        .fluid-row .col-6,
        .md-fluid-row .col-6 { width: 50% !important;}
        .fluid-row .col-7,
        .md-fluid-row .col-7 { width: 57.75% !important;}
        .fluid-row .col-8,
        .md-fluid-row .col-8 { width: 66% !important;}
        .fluid-row .col-9,
        .md-fluid-row .col-8 { width: 74.25% !important;}
        .fluid-row .col-10,
        .md-fluid-row .col-10 { width: 82.5% !important; }
        .fluid-row .col-11,
        .md-fluid-row .col-11 { width: 90.75% !important;}
        /* ========== end scaffolding ========== */

        .banner {
            padding: 30px;
        }

        .brand-work {
            padding: 10px;
        }

        .brand-benefits p {
            margin: 15px 0;
            font-size: 16px;
        }

        .brand-work p {
            font-size: 15px;
        }

        .brand-benefits img {
            height: auto;
        }

        .brand-referral p {
            font-size: 15px;
            margin: 10px 0 0 0;
        }

        .brand-referral .action {
            margin: 30px 0;
        }

        .brand-follow {
            padding: 35px 5px;
        }

        .refer-friend p {
            margin: 20px;
            text-align: center;
        }

        .refer-friend, .refer-friend h2, .refer-friend-content h2, .refer-friend-content p {
            text-align: center;
        }

        .refer-friend-content h2 br {
            display: none;
        }

        .brand-follow-icons a {
            margin-right: 5px;
        }

        /* ========== custom styles ============= */
        }

    </style>
    <style type="text/css" media="only screen and (max-width: 481px)">
      @media screen and (max-width: 481px) {
        .md-fluid-row .col-1,
        .md-fluid-row .col-2,
        .md-fluid-row .col-3,
        .md-fluid-row .col-4,
        .md-fluid-row .col-5,
        .md-fluid-row .col-6,
        .md-fluid-row .col-7,
        .md-fluid-row .col-8,
        .md-fluid-row .col-9,
        .md-fluid-row .col-10,
        .md-fluid-row .col-11 {
          width: 100% !important;
        }
      }
    </style>
    <!-- end responsive styles -->
  </head>
